{
  "apps": [
    {
      "name": "lookFor",
      "cwd": "./",
      "script":"pm2-server.js",
      "log_date_format": "YYYY-MM-DD HH:mm Z",
      "error_file": "./logs/app-err.log",
      "out_file": "./logs/app-out.log",
      "exec_mode":"fork",
      "watch": true,
      "ignore_watch": "./logs",
      "env": {
        "COMMON_VARIABLE": "true"
      },
      "env_production" : {
        "NODE_ENV": "production"
      }
    }
  ]
}